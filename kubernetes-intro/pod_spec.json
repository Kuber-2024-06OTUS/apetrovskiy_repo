{
    "automountServiceAccountToken": false,
    "containers": [
        {
            "image": "nginx:alpine",
            "imagePullPolicy": "IfNotPresent",
            "lifecycle": {
                "preStop": {
                    "exec": {
                        "command": [
                            "rm -f /homework/index.html"
                        ]
                    }
                }
            },
            "name": "web",
            "ports": [
                {
                    "containerPort": 8000,
                    "hostPort": 8000,
                    "name": "web-port",
                    "protocol": "TCP"
                }
            ],
            "resources": {},
            "terminationMessagePath": "/dev/termination-log",
            "terminationMessagePolicy": "File",
            "volumeMounts": [
                {
                    "mountPath": "/homework",
                    "name": "webvolume"
                }
            ]
        }
    ],
    "dnsPolicy": "ClusterFirst",
    "enableServiceLinks": true,
    "initContainers": [
        {
            "args": [
                "echo 'Hello World!' \u003e /init/index.html"
            ],
            "command": [
                "sh",
                "-c"
            ],
            "image": "busybox:stable",
            "imagePullPolicy": "IfNotPresent",
            "name": "init-web",
            "resources": {},
            "terminationMessagePath": "/dev/termination-log",
            "terminationMessagePolicy": "File",
            "volumeMounts": [
                {
                    "mountPath": "/init",
                    "name": "webvolume"
                }
            ]
        }
    ],
    "nodeName": "minikube",
    "os": {
        "name": "linux"
    },
    "preemptionPolicy": "PreemptLowerPriority",
    "priority": 0,
    "restartPolicy": "Never",
    "schedulerName": "default-scheduler",
    "securityContext": {},
    "serviceAccount": "homework",
    "serviceAccountName": "homework",
    "terminationGracePeriodSeconds": 30,
    "tolerations": [
        {
            "effect": "NoExecute",
            "key": "node.kubernetes.io/not-ready",
            "operator": "Exists",
            "tolerationSeconds": 300
        },
        {
            "effect": "NoExecute",
            "key": "node.kubernetes.io/unreachable",
            "operator": "Exists",
            "tolerationSeconds": 300
        }
    ],
    "volumes": [
        {
            "emptyDir": {},
            "name": "webvolume"
        }
    ]
}